{#
/**
 * @file observation.html.twig
 * Default theme implementation to present Observation data.
 *
 * This template is used when viewing Observation pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_observation()
 *
 * @ingroup themeable
 */
#}

{{ kint() }}

<div{{ attributes.addClass('observation') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
